{
  "name": "@carenow/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "database:generate": "prisma generate --schema=./prisma/schema.prisma",
    "database:migration": "prisma migrate dev --schema=./prisma/schema.prisma",
    "database:push": "prisma db push",
    "database:reset": "prisma migrate reset --schema=./prisma/schema.prisma",
    "database:seed": "prisma db seed",
    "database:setup": "pnpm database:push && pnpm database:seed",
    "start:api": "nx run api:serve --watch"
  },
  "private": true,
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/eslint": "20.6.2",
    "@nx/eslint-plugin": "20.6.2",
    "@nx/jest": "20.6.2",
    "@nx/js": "20.6.2",
    "@nx/nest": "20.6.2",
    "@nx/node": "20.6.2",
    "@nx/web": "20.6.2",
    "@nx/webpack": "20.6.2",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "~0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "~18.16.9",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "nx": "20.6.2",
    "prettier": "^2.6.2",
    "prisma": "^6.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.19.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^10.0.2",
    "@nestjs/platform-express": "^10.0.2",
    "@prisma/client": "^6.5.0",
    "axios": "^1.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
